programa {
  funcao inicio() 
  {

  real x, y

  escreva("Digite o primeiro valor: ")
  leia(x)
  escreva("\n")
  escreva("\n")

  escreva("Digite o segundo valor: ")
  leia(y)
  escreva("\n")
  escreva("\n")

  escreva("O primeiro número é maior que o segundo: ", x>y)
  escreva("\n")
  escreva("O primeiro número é menor que o segundo: ", x<y)
  escreva("\n")
  escreva("O primeiro número é igual ao segundo: ", x==y)


  }
}
